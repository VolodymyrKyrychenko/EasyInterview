@model Web.Models.LibraryModels.LibraryViewModel

<div class="container">
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#home">@Model.Default.Name</a></li>
        <li><a data-toggle="tab" href="#menu1">@Model.Library.Name</a></li>
    </ul>

    <div class="tab-content">
        <div id="home" class="tab-pane fade in active">
            <div class="row">
                @await Html.PartialAsync("Details", Model.Default.Problems.Select(p => p.Problem))
            </div>
        </div>
        <div id="menu1" class="tab-pane fade">
            @await Html.PartialAsync("Details", Model.Library.Problems.Select(p => p.Problem))
        </div>
    </div>
</div>